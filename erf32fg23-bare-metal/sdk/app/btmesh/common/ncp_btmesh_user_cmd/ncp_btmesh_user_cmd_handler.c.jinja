{{ autogenerated_c_header }}

#include <stdbool.h>
{%- for entry in ncp_cmd_to_target %}
{%-   if entry["include"] is string %}
#include "{{ entry["include"] }}"
{%-   else %}
{%-     for f in entry["include"] %}
#include "{{ f }}"
{%-     endfor %}
{%-   endif %}
{%- endfor %}

void ncp_cmd_to_target_handler(void *data, bool *cmd_handled)
{
  (void) data;
  (void) cmd_handled;
  {%- for entry in ncp_cmd_to_target %}
  {{ entry["function"] }}(data, cmd_handled);
  {%- endfor %}
}
