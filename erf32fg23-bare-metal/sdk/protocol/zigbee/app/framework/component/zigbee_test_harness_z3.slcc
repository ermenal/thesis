package: Zigbee
description: This component contains the functionality necessary for creating a Zigbee 3.0 test harness application.
label: Zigbee 3.0 Test Harness
quality: production
id: zigbee_test_harness_z3
category: Zigbee|Utility|Testing
provides:
  - name: zigbee_test_harness_z3
requires:
  - name: zigbee_zll_profile
  - name: zigbee_network_steering
  - name: zigbee_packet_handoff
  - name: zigbee_zll_commissioning_common
  - name: zigbee_interpan
  - name: zigbee_address_table
  - name: zigbee_end_device_support
source:
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-core.c
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-mac.c
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-nwk.c
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-aps.c
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-zll.c
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-zdo.c
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-ota-server.c
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3/test-harness-z3-zcl.c
include:
  - path: protocol/zigbee/app/framework/plugin/test-harness-z3
    file_list:
      - path: test-harness-z3-core.h
      - path: test-harness-z3-mac.h
      - path: test-harness-z3-nwk.h
      - path: test-harness-z3-zdo.h
      - path: test-harness-z3-zll.h
template_contribution:
  - name: component_catalog
    value: zigbee_test_harness_z3
  - name: zigbee_af_callback
    value:
      callback_type: event_init
      function_name: sli_zigbee_af_test_harness_z3_init_callback
  - name: zigbee_stack_callback
    value:
      callback_type: stack_status
      function_name: emberAfPluginTestHarnessZ3StackStatusCallback
  - name: cli_group
    value:
      name: core
      help: core SAP interfaces.
  - name: cli_command
    value:
      group: core
      name: apsde-req
      help: Sends out a request based on the input parameters.
      handler: sl_zigbee_test_harness_z3_apsde_data_request
      argument:
        - type: uint8
          help: Dest Mode 0=BOUND(Bind entry needed), 1=Group, 2=ShortAddress, 3=EUI64(address table entry needed).
        - type: hex
          help: Dest Address in Octate string of 2 bytes or 8 bytes in little endian format.
        - type: uint8
          help: Dest Endpoint.
        - type: uint16
          help: Profile Id.
        - type: uint16
          help: Cluster Id.
        - type: uint8
          help: src Endpoint.
        - type: uint8
          help: ASDU Length.
        - type: hex
          help: ASDU.
        - type: uint8
          help: txOption Bit(0)=Encryption, Bit(2)=ACK , rest not implemented.
        - type: uint8
          help: Use Alias for Source Address.
        - type: uint16
          help: Alias Address.
        - type: uint8
          help: Alias Sequence Number.
        - type: uint8
          help: Radius- only used for Alias now.
  - name: cli_command
    value:
      group: core
      name: nlde-req
      help: Sends out a request based on the input parameters at NLDE layer
      handler: sl_zigbee_test_harness_z3_nlde_data_request
      argument:
        - type: uint8
          help: Dest Mode 2=16 bit dest address.
        - type: uint16
          help: Dest 16-bit Address.
        - type: uint8
          help: NSDU Length.
        - type: hex
          help: NSDU Octate String.
        - type: uint16
          help: NSDU Handle.
        - type: uint8
          help: Use Alias for Source Address.
        - type: uint16
          help: Alias source 16-bit address.
        - type: uint8
          help: Alias sequence number.
        - type: uint8
          help: The distance in hops.
        - type: uint8
          help: The distance in hops for a multicast by the nodes belonging to the group.
        - type: uint16
          help: Discover route , 0x00=Suppress, 0x01=Enable Route discovery.
        - type: uint8
          help: Enable NWK layer security based on NIB attribute nwkSecurityLevel.
  - name: cli_command
    value:
      group: core
      name: mcpsd-req
      help: Sends out a request based on the input parameters at MAC layer
      handler: sl_zigbee_test_harness_z3_mcps_data_request
      argument:
        - type: uint8
          help: Source Mode  0=No Address, 1=RESERVE, 2= 16 Bit Short, 3=EUI64.
        - type: uint16
          help: Source PAN Id.
        - type: hex
          help: Source Address in Octate string of 2 bytes or 8 bytes in little endian format..
        - type: uint8
          help: Destination mode
        - type: uint16
          help: Destination PAN Id.
        - type: hex
          help: Destination address.
        - type: uint8
          help: MSDU Length.
        - type: hex
          help: MSDU Octate String.
        - type: uint8
          help: MSDU Handle.
        - type: uint8
          help: txOption Bit(0)=Ack, Bit(1)=GST, Bit(2)=Indirect, Bit(3)=Security.
