<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.preferencePages">
      <page name="TCP/IP Adapters"
        category="com.silabs.ss.platform.internal.device.ui.preference.DeviceManagerPreferencePage"
     	class="com.silabs.ss.platform.internal.apack.ip.ui.UdpDiscoveryPreferencePage"
     	id="com.silabs.ss.platform.internal.apack.ip.ui.UdpDiscoveryPreferencePage">
     </page> 
   </extension>
   <extension
      point="com.silabs.ss.platform.device.core.deviceLocator">
      <locator
            description="InSight Ethernet UDP Discovery"
            enabled="true"
            locatorClass="com.silabs.ss.platform.api.apack.ip.EthernetDiscovery"
            preferencePageId="com.silabs.ss.platform.internal.apack.ip.ui.UdpDiscoveryPreferencePage">
      </locator>
      <locator
            description="Segger J-Link Ethernet UDP Discovery"
            enabled="false"
            locatorClass="com.silabs.ss.platform.api.apack.ip.JLinkEthernetDiscovery"
            preferencePageId="com.silabs.ss.platform.internal.apack.ip.ui.UdpDiscoveryPreferencePage">
      </locator>
   </extension>
   <extension
        point="com.silabs.ss.platform.device.core.deviceDomain">
     <domain
           description="ISD Adapter Bridge Domain (temporary)"
           id="isdAdapter">
     </domain>
  </extension>
   <extension
         point="com.silabs.ss.platform.device.core.deviceCreator">
      <creator
            class="com.silabs.ss.platform.api.apack.ip.EthernetDeviceCreator">
      </creator>
   </extension>
</plugin>
