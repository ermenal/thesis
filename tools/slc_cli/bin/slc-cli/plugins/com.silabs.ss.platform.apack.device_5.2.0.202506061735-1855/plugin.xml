<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.silabs.ss.platform.device.core.deviceLocator">
      <locator
            description="Adapter Pack Locator"
            enabled="true"
            locatorClass="com.silabs.ss.platform.internal.apack.device.AdapterPackDetectionLocator">
      </locator>
   </extension>
   <extension
         point="com.silabs.ss.platform.device.core.deviceNotifier">
      <notifier
            description="USB Hotplug Notifier"
            enabled="true"
            notifierClass="com.silabs.ss.platform.internal.apack.device.UsbHotplugNotifier">
      </notifier>
   </extension>
   <extension
         point="com.silabs.ss.platform.device.core.deviceDomain">
      <domain
            description="USB Hotplug devices"
            id="usbNotifier">
      </domain>
   </extension>
   <extension
         point="com.silabs.ss.platform.device.core.deviceCreator">
      <creator
            class="com.silabs.ss.platform.internal.apack.device.UsbDeviceCreator">
      </creator>
   </extension>
   <extension
         point="com.silabs.ss.platform.device.core.deviceInspector">
      <inspector
            class="com.silabs.ss.platform.internal.apack.device.UsbVidPidInspector"
            description="USB VID/PID based inspector"
            id="com.silabs.ss.platform.internal.apack.device.UsbVidPidInspector"
            triggerUponDetection="true">
      </inspector>
   </extension>
</plugin>
